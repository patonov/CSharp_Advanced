using System;  
using System.Collections.Generic;  
using System.Linq;  
					
public class Program
{
	public static void Main()
	{
	
		int armor = int.Parse(Console.ReadLine());
        int n = int.Parse(Console.ReadLine());
		
		char[,] matrix = new char[n, n];
        
		for (int row = 0; row < n; row++)
        {
        	string currentRow = Console.ReadLine();
        	for (int col = 0; col < n; col++)
            {
            	matrix[row, col] = currentRow[col];
            }
        }
		
		string command = Console.ReadLine();
		
		while (true)
		{
		string[] commandArr = command.Split().ToArray();
		string direction = commandArr[0];
		int moveRow = int.Parse(commandArr[1]);
		int moveCol = int.Parse(commandArr[2]);
		
			
		command = Console.ReadLine();	
		}
		
	
	
		
		for (int row = 0; row < n; row++)
        {
           for (int col = 0; col < n; col++)
           {
           Console.Write(matrix[row, col]);
           }
           Console.WriteLine();
        }
		
	}

	public static bool IsValid(int row, int col, int n)
        {
            if (row < 0 || row >= n)
            {
                return false;
            }
            if (col < 0 || col >= n)
            {
                return false;
            }
            return true;
        }

        public static char Move(string movement, int row, int col, char[,] matrix)
        {
            if (movement == "up")
            {
                return matrix[row, col];
            }
            else if (movement == "down")
            {
                return matrix[row, col];
            }
            else if (movement == "right")
            {
                return matrix[row, col];
            }
            else if (movement == "left")
            {
                return matrix[row, col];
            }
			return matrix[row, col];
			
        }
    
}
